package com.yedam.print;

public class PrintExample {
	public static void main(String[] args) {
		//println
		System.out.println("출력과 동시에 행을 변경(Enter)");
		//print
		System.out.print("출력만 함.");
		//printf
		System.out.printf("이름 : %s, 나이 : %d, 온도 : %f","김또치",20,9.5); 
		
		//printf 메소드 사용법
		int value = 123;
		System.out.printf("상품의 가격: %d원",value);
		System.out.printf("상품의 가격: %6d원",value);
		System.out.printf("상품의 가격: %-6d원",value);
		System.out.printf("상품의 가격: %06d원",value);
	}
}
